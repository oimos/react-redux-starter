{
  "extends": "eslint-config-airbnb-base",
  "env": {
    "browser": true
  },
  "rules": {
    "import/prefer-default-export": 0,
    "linebreak-style": 0,
    "no-param-reassign": 0,
    "no-unused-vars": 0,
    "no-console": 0,
    "no-plusplus": 0,
    "no-else-return": 0,
    "no-underscore-dangle": 0
  },
  "globals": {
    "window": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
    "ecmaFeatures": {
      "jsx": true,
      "modules": true,
      "experimentalObjectRestSpread": true
    }
  }
}
